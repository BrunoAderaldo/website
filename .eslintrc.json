{
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "globals": {
    "graphql": true
  },
  "rules": {
    "react/prop-types": false,
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "camelcase": "off",
    "prettier/prettier": "error"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["prettier", "react-hooks"]
}
